<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My ASP.NET Application</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/LauOutPages/oayoutpages.css" rel="stylesheet" />
    <link href="~/Content/css/Patient/Patient.css" rel="stylesheet" />
    <link href="~/Content/css/Patient/Login.css" rel="stylesheet" />
    <link href="~/Content/css/Patient/Appoimentpage.css" rel="stylesheet" />
    <link href="~/Content/css/HomePages/IndexPage.css" rel="stylesheet" />
    <link href="~/Content/css/HomePages/IndexPage1.css" rel="stylesheet" />

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>




</head>
<body>

    <nav class="navbar bg-light navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header logo">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                    <span><i class="fa fa-bars" aria-hidden="true" style="font-size:30px"></i></span>

                </button>
                <a class="navbar-brand" href="#">HEALTH</a>
            </div>
            <div class="collapse navbar-collapse myNav" id="myNavbar">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="/Home/Index/">Home</a></li>

                    <li><a href="#">information</a></li>
                    <li><a href="/Home/Appointmentpage/">Appointment</a></li>
                    <li><a href="#">Team</a></li>
                    <li><a href="#">about</a></li>
                    <li><a href="#">Contact</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right">

                    @if (Session["patientUsername"] != null)
                    {
                        <li><a href="#"><span>@Session["patientUsername"].ToString()</span></a></li>
                        <li><a href="/Login/LogOut"><span class="glyphicon glyphicon-log-in"></span> LogOut</a></li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" style="border-bottom:none;" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-ellipsis-v" style="font-size:25px;" aria-hidden="true"></i></a>
                            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item" style="border-bottom:none;" href="/Patient/Patientprofile/">Profile</a><br/>
                                <a class="dropdown-item" style="border-bottom:none;" href="/Appoiment/patientcheckAppoiment/">Appoiment Check</a><br/>
                                <a class="dropdown-item" style="border-bottom:none;" href="#">Action</a>

                            </div>
                        </li>

                    }
                    else
                    {
                        <li><a href="/Patient/PatientRegistration/"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
                        <li><a href="/Login/LoginPage"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>

                    }

                </ul>
            </div>
        </div>
    </nav>

    <br /><br /><br />
    <div class="container-fluid">
        @RenderBody()
    </div>
    <div class="footer">
        <div class="container-fluid">
            
            <div class="row text-center" style="background-color:#33FF00">
                <div class="col-lg-4 col--md-4 col-sm-12"></div>
                <div class="col-lg-4 col--md-4 col-sm-12">
                    <div class="">
                        <span class="icons"><i class="fa fa-facebook fonticons" aria-hidden="true"></i></span>
                        <span class="icons"><i class="fa fa-youtube-play fonticons" aria-hidden="true"></i></span>
                        <span class="icons"><i class="fa fa-instagram fonticons" aria-hidden="true"></i></span>
                        <span class="icons"><i class="fa fa-twitter fonticons" aria-hidden="true"></i></span>
                        <span class="icons"><i class="fa fa-whatsapp fonticons" aria-hidden="true"></i></span>
                    </div>
                </div>
                <div class="col-lg-4 col--md-4 col-sm-12"></div>
            </div>
        </div>
    </div>

    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

    <script src="~/Scripts/js/patient.js"></script>
    <script src="~/Scripts/js/Login.js"></script>
   
    <script src="~/Scripts/js/Appoimentpage.js"></script>
    <script src="~/Scripts/js/ToJsDate.js"></script>
</body>
</html>

<!DOCTYPE html>


<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link href="~/Content/AllCssFile/UserAccountCreateCssfile.css" rel="stylesheet" />
    <link href="~/Content/AllCssFile/UserLoginPage.css" rel="stylesheet" />
    <link href="~/Content/AllCssFile/MemberPage.css" rel="stylesheet" />
    <link href="~/Content/AllCssFile/Media.css" rel="stylesheet" />
</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-default bg-dark">
        <a class="navbar-brand" href="#">Navbar</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Link</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link" href="/Home/Media/">Media</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link" href="/Admin/AdminMasterPage/">Admin</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link" href="/MemberShip/MemberPage/">MemberShip</a>
                </li>


            </ul>
            <ul class="nav navbar-nav">

                @if (Session["UserName"] != null)
                {

                    <li class="nav-item">
                        <a class="nav-link" href="#"><span>Hi </span>@Session["UserName"].ToString() </a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="/Login/UserLogout/"><i class="fa fa-sign-in" aria-hidden="true"></i> Logout</a>
                    </li>

                }
                else
                {
                    <li class="nav-item">
                        <a class="nav-link" href="/User/UserAccountCreatePage/"><i class="fa fa-user" aria-hidden="true"></i> Sign Up</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="/Login/UserLoginPage/"><i class="fa fa-sign-in" aria-hidden="true"></i> Login</a>
                    </li>
                }


            </ul>
        </div>
    </nav>


    <br>
    <div class="container-fluid">

        @RenderBody()


    </div>

    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="~/Scripts/AllJqueryFile/UserAccountCreateJsfile.js"></script>
    <script src="~/Scripts/AllJqueryFile/UserLoginPage.js"></script>
    <script src="~/Scripts/AllJqueryFile/Memberview.js"></script>
    <script src="~/Scripts/AllJqueryFile/Media.js"></script>
</body>
</html>